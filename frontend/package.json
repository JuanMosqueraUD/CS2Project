{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "packageManager": "npm@10.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "electron": "electron ./electron/main.cjs",
    "start": "npm run build && npm run electron",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "vis-network": "^10.0.2",
    "vue": "^3.5.18",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.24",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^39.2.4",
    "electron-builder": "^24.13.3",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  },
  "build": {
    "appId": "com.tuapp.id",
    "productName": "CS2Project",
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
